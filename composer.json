{
    "name": "poirot/skeleton"
    , "description": "Poirot Skeleton Application."
    , "keywords": [
        "application framework"
        , "poirot"
        , "raya media"
    ]
    , "authors": [
        {
            "name": "Payam Naderi"
            , "email": "naderi.payam@gmail.com"
        }
    ]
    , "config":
    {
        "vendor-dir": "./vendor"
        , "bin-dir"   : "./vendor/bin"
    }
    , "require": {
        "poirot/loader": "dev-devel"
        , "poirot/std": "dev-devel"
        , "poirot/ioc": "dev-devel"
        , "poirot/application": "dev-devel"
        , "poirot/cor-foundation": "dev-devel"
        , "poirot/cor-http_foundation": "dev-devel"
        , "poirot/cor-http_renderer": "dev-devel"
        , "poirot/cor-cli_foundation": "dev-devel"
    }
    ,"require-dev": {
       "mmucklo/krumo": "^0.5.0"
    }
    , "minimum-stability": "dev"
    , "prefer-stable" : true

}
